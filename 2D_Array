#zeros() ones()
import numpy as np

y=np.ones((2,3))
print(y)

x=np.zeros((2,3))
print(x)

z=np.ones((3,3),dtype=[('m','int'),('n','float')])
print(z)
#first is int and second one is float

a=np.array([
    [10,20,30],
    [40,50,60],
    [70,80,90]
])
for i in a:
    for j in i:
        print(j,end=' ')
    print()

print("Row 0 ",a[0])
print("Row 0 ",a[1])

print(a[0:2])

print(a[1:])

print(a[::-1])

print((a[0:-1]))
